{
  "name": "aragami",
  "description": "Another cache ORM.",
  "version": "1.2.5",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint --fix .",
    "build": "rimraf dist && tsc",
    "test": "jest --passWithNoTests",
    "start": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://code.mycard.moe/3rdeye/aragami.git"
  },
  "author": "Nanahira <nanahira@momobako.com>",
  "license": "MIT",
  "keywords": [],
  "bugs": {
    "url": "https://code.mycard.moe/3rdeye/aragami/issues"
  },
  "homepage": "https://code.mycard.moe/3rdeye/aragami",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "tests",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@types/jest": "^28.1.8",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.7.23",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@sesamecare-oss/redlock": "^1.4.0",
    "better-lock": "^2.0.3",
    "class-transformer": "^0.5.1",
    "encoded-buffer": "^0.2.6",
    "generic-pool": "^3.9.0",
    "ioredis": "^5.2.3",
    "lodash": "^4.17.21",
    "lru-cache": "^7.13.1",
    "typed-reflector": "^1.0.11"
  }
